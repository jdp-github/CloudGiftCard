/* pages/customserv/customserv.wxss */

.contact-container {
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    background: #f7f7f7;
    position: relative;
    display: flex;
    justify-content: center;
    align-items: middle;
    margin: auto;
}

.img-background {
    width: 100%;
    /* height: 2000rpx; */
}

.contact-btn {
    width: 70%;
    position: absolute;
    margin: auto;
    float: left;
    bottom: 180rpx;
    background: #f33;
    color: white;
    border-radius: 50rpx;
}
